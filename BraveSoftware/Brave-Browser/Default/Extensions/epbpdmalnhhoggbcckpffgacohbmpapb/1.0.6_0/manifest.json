{
   "action": {
      "default_icon": {
         "19": "icons/dr_active_19.png",
         "38": "icons/dr_active_38.png"
      },
      "default_popup": "popup.html",
      "default_title": "__MSG_titleDefault__"
   },
   "background": {
      "service_worker": "javascript/worker.js"
   },
   "commands": {
      "darkModeToggle": {
         "description": "Dark Mode On/Off",
         "suggested_key": {
            "default": "Alt+Shift+D",
            "mac": "Alt+Shift+D",
            "windows": "Alt+Shift+D"
         }
      },
      "nightModeToggle": {
         "description": "Night Shift On/Off",
         "suggested_key": {
            "default": "Alt+Shift+S",
            "mac": "Alt+Shift+S",
            "windows": "Alt+Shift+S"
         }
      },
      "toggle": {
         "description": "Extension On/Off",
         "suggested_key": {
            "default": "Alt+Shift+E",
            "mac": "Alt+Shift+E",
            "windows": "Alt+Shift+E"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "javascript/content.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_extDescription__",
   "host_permissions": [ "*://*/*", "*://*/*/*" ],
   "icons": {
      "128": "icons/dr_128.png",
      "16": "icons/dr_16.png",
      "48": "icons/dr_48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_extName__",
   "permissions": [ "fontSettings", "activeTab", "storage", "tabs", "alarms" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.6"
}
